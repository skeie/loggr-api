    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "express-jwt": "^5.1.0",
    "express-validator": "^2.20.8",
    "install": "^0.8.7",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.15.0",
    "method-override": "^2.3.5",
    "morgan": "~1.5.1",
    "multer": "^1.3.0",
    "node-fetch": "^1.6.3",
    "npm": "^4.2.0",
    "pg-promise": "^3.8.2",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
