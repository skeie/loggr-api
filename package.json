{
  "name": "loggr-api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=prod nodemon ./server/app --exec babel-node",
    "debug": "NODE_ENV=prod nodemon --inspect ./server/app --exec babel-node",
    "clean": "rm -rf build && mkdir build",
    "build-server": "./node_modules/.bin/babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-server",
    "start": "NODE_ENV=prod node ./build/app.js"
    
  },
  "dependencies": {
    "async": "^2.0.1",
    "body-parser": "~1.12.0",
    "cookie-parser": "~1.3.4",
    "debug": "~2.1.1",
    "express": "~4.12.2",
    "express-validator": "^2.20.8",
    "lodash": "^4.15.0",
    "method-override": "^2.3.5",
    "morgan": "~1.5.1",
    "pg-promise": "^3.8.2",
    "serve-favicon": "~2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "eslint": "^3.4.0",
    "eslint-config-google": "^0.6.0",
    "nodemon": "^1.10.2"
  },
  "engines": {
    "node": "6.5.0"
  },
  "main": "./bin/www ",
  "author": "Truls Skeie",
  "license": "ISC"
}
